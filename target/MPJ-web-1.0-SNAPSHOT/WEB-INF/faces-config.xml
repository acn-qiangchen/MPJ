<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
              version="2.2">
    <application>
        <locale-config>
            <default-locale>ja</default-locale>
        </locale-config>
    </application>
    <navigation-rule>
        <navigation-case>
            <from-outcome>welcome</from-outcome>
            <to-view-id>/Main.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>plan_create</from-outcome>
            <to-view-id>/plan/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>plan_list</from-outcome>
            <to-view-id>/plan/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>plan_edit</from-outcome>
            <to-view-id>/plan/Edit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>plan_detail</from-outcome>
            <to-view-id>/plan/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>ticket_create</from-outcome>
            <to-view-id>/ticket/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>ticket_list</from-outcome>
            <to-view-id>/ticket/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>ticket_edit</from-outcome>
            <to-view-id>/ticket/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>ticket_detail</from-outcome>
            <to-view-id>/ticket/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>welcome</from-outcome>
            <to-view-id>/welcomeJSF.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>purchase</managed-bean-name>
        <managed-bean-class>com.tink.mpj.app.bean.PurchaseController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>purchaseJpa</managed-bean-name>
        <managed-bean-class>com.tink.mpj.business.ejb.PurchaseFacade</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <converter>
        <converter-for-class>com.tink.mpj.persistence.model.Purchase</converter-for-class>
        <converter-class>com.tink.mpj.app.bean.PurchaseConverter</converter-class>
    </converter>
    <navigation-rule>
        <navigation-case>
            <from-outcome>purchase_create</from-outcome>
            <to-view-id>/purchase/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>purchase_list</from-outcome>
            <to-view-id>/purchase/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>purchase_edit</from-outcome>
            <to-view-id>/purchase/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>purchase_detail</from-outcome>
            <to-view-id>/purchase/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>member</managed-bean-name>
        <managed-bean-class>com.tink.mpj.app.bean.MemberController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>memberJpa</managed-bean-name>
        <managed-bean-class>com.tink.mpj.business.ejb.MemberFacade</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <converter>
        <converter-for-class>com.tink.mpj.persistence.model.Member</converter-for-class>
        <converter-class>com.tink.mpj.app.bean.MemberConverter</converter-class>
    </converter>
    <navigation-rule>
        <navigation-case>
            <from-outcome>member_create</from-outcome>
            <to-view-id>/member/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>member_list</from-outcome>
            <to-view-id>/member/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>member_edit</from-outcome>
            <to-view-id>/member/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>member_detail</from-outcome>
            <to-view-id>/member/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>